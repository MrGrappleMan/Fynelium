[org/gnome/desktop/interface]
clock-show-seconds=false
enable-animations=true
cursor-blink=false
clock-show-weekday=true
clock-show-date=true
font-hinting='full'
font-antialiasing='grayscale'
color-scheme='prefer-dark'
locate-pointer=false

[org/gnome/desktop/peripherals/mouse]
accel-profile='adaptive'
speed=1.0

[org/gnome/software]
download-updates=false
allow-updates=true
download-updates-notify=false
show-ratings=true
show-upgrade-prerelease=true
show-nonfree-ui=true
show-only-free-apps=false
show-only-verified-apps=false
prompt-for-nonfree=true
refresh-when-metered=false

[org/gnome/shell/app-switcher]
current-workspace-only=true

[org/gnome/system/location]
max-accuracy-level='exact'
enabled=true

[org/gnome/login-screen]
allowed-failures=20
fallback-logo=''
logo=''

[org/gnome/SessionManager]
auto-save-session=true
logout-prompt=true
auto-save-session-one-shot=true

[org/gnome/mutter]
dynamic-workspaces=true
center-new-windows=true
auto-maximize=true

[org/gnome/desktop/sound]
allow-volume-above-100-percent=true

[org/gnome/desktop/thumbnail-cache]
maximum-size=4096
maximum-age=360

[org/gnome/desktop/background]
picture-opacity=0

[org/gnome/desktop/lockdown]
disable-lock-screen=false

[org/gnome/online-accounts]
whitelisted-providers=['all']

[org/gnome/desktop/a11y/interface]
high-contrast=false

[org/freedesktop/Tracker3/Miner/Files]
crawling-interval=0
enable-monitors=true
index-on-battery=false
index-on-battery-first-time=true
index-removable-devices=true
initial-sleep=1
throttle=5

[org/gnome/settings-daemon/plugins/power]
sleep-inactive-battery-type='hibernate'
sleep-inactive-ac-type='nothing'
power-saver-profile-on-low-battery=true
idle-dim=true

[org/gnome/desktop/session]
idle-delay=60

[org/gnome/desktop/screensaver]
idle-activation-enabled=false
lock-delay=300

[org/gnome/desktop/peripherals/keyboard]
remember-numlock-state=true

[org/gnome/desktop/break-reminders/eyesight]
fade-screen=true
notify=true
interval-seconds=1200
countdown=false
delay-seconds=10
duration-seconds=20
play-sound=true
notify-overdue=true
notify-upcoming=true

[org/gnome/desktop/break-reminders/movement]
notify=true
notify-overdue=true
notify-upcoming=true
play-sound=true
countdown=false
